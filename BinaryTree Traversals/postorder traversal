class Solution {
    List<Integer> result =new ArrayList<Integer>();
    public List<Integer> postorderTraversal(TreeNode root) {
        if(root==null)
           return result;
        traverse(root);
        return result;
    }
    public void traverse(TreeNode root){
        if(root.left!=null)
           traverse(root.left);
        if(root.right!=null)
           traverse(root.right);
        result.add(root.val);
    }
}
